<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.program.ProgramTempOwner" metadata-complete="true">
        <table name="programtempowner"/>
        <dynamic-insert>false</dynamic-insert>
        <dynamic-update>false</dynamic-update>
        <select-before-update>false</select-before-update>
        <batch-size>-1</batch-size>
        <mutable>true</mutable>
        <optimistic-lock>version</optimistic-lock>
        <polymorphism>implicit</polymorphism>
        <attributes>
            <id name="id">
                <column name="programtempownerid"/>
            </id>
            <basic name="validTill" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="reason" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <many-to-one name="program" target-entity="org.hisp.dhis.program.Program" fetch="LAZY" fetch-mode="SELECT" optional="true">
                <join-column name="programid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_programtempowner_programid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="trackedEntity" target-entity="org.hisp.dhis.trackedentity.TrackedEntity" fetch="LAZY" fetch-mode="SELECT" optional="true">
                <join-column name="trackedentityid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_programtempowner_trackedentityinstanceid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="user" target-entity="org.hisp.dhis.user.User" fetch="LAZY" fetch-mode="SELECT" optional="true">
                <join-column name="userid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_programtempowner_userid"/>
                <cascade/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
