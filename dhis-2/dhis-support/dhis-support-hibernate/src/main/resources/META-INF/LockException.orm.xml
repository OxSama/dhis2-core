<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.dataset.LockException" metadata-complete="true">
        <table name="lockexception"/>
        <dynamic-insert>false</dynamic-insert>
        <dynamic-update>false</dynamic-update>
        <select-before-update>false</select-before-update>
        <batch-size>-1</batch-size>
        <mutable>true</mutable>
        <optimistic-lock>version</optimistic-lock>
        <polymorphism>implicit</polymorphism>
        <attributes>
            <id name="id">
                <column name="lockexceptionid"/>
            </id>
            <basic name="created" fetch="EAGER" optional="false" optimistic-lock="true"/>
            <many-to-one name="organisationUnit" target-entity="org.hisp.dhis.organisationunit.OrganisationUnit" fetch="LAZY" fetch-mode="SELECT" optional="true">
                <join-column name="organisationunitid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_lockexception_organisationunitid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="period" target-entity="org.hisp.dhis.period.Period" fetch="LAZY" fetch-mode="SELECT" optional="true">
                <join-column name="periodid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_lockexception_periodid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="dataSet" target-entity="org.hisp.dhis.dataset.DataSet" fetch="LAZY" fetch-mode="SELECT" optional="true">
                <join-column name="datasetid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_lockexception_datasetid"/>
                <cascade/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
