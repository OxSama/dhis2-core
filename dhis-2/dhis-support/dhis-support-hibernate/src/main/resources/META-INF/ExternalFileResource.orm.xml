<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.fileresource.ExternalFileResource" metadata-complete="true" cacheable="true">
        <table name="externalfileresource"/>
        
        <attributes>
            <id name="id">
                <column name="externalfileresourceid"/>
            </id>
            <basic name="uid" fetch="EAGER" optional="false">
                <column name="uid" unique="true" nullable="false" length="11"/>
            </basic>
            <basic name="code" fetch="EAGER" optional="true">
                <column name="code" unique="true" nullable="true" length="50"/>
            </basic>
            <basic name="created" fetch="EAGER" optional="false"/>
            <basic name="lastUpdated" fetch="EAGER" optional="false"/>
            <basic name="accessToken" fetch="EAGER" optional="true"/>
            <basic name="expires" fetch="EAGER" optional="true"/>
            <many-to-one name="lastUpdatedBy" target-entity="org.hisp.dhis.user.User" fetch="LAZY" optional="true">
                <join-column name="lastupdatedby" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_lastupdateby_userid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="fileResource" target-entity="org.hisp.dhis.fileresource.FileResource" fetch="LAZY" optional="false">
                <join-column name="fileresourceid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_fileresource_externalfileresource"/>
                <cascade/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
