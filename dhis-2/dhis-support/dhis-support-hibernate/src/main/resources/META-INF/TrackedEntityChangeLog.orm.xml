<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.trackedentity.TrackedEntityChangeLog" metadata-complete="true">
        <table name="trackedentityaudit"/>
        <dynamic-insert>false</dynamic-insert>
        <dynamic-update>false</dynamic-update>
        <select-before-update>false</select-before-update>
        <batch-size>-1</batch-size>
        <mutable>true</mutable>
        <optimistic-lock>version</optimistic-lock>
        <polymorphism>implicit</polymorphism>
        <attributes>
            <id name="id">
                <column name="trackedentityauditid"/>
            </id>
            <basic name="trackedEntity" fetch="EAGER" optional="true" optimistic-lock="true">
                <column name="trackedEntity" unique="false"/>
            </basic>
            <basic name="created" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="accessedBy" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="auditType" fetch="EAGER" optional="false" optimistic-lock="true">
                <column name="audittype" unique="false" nullable="false" length="100"/>
            </basic>
            <basic name="comment" fetch="EAGER" optional="true" optimistic-lock="true"/>
        </attributes>
    </entity>
</entity-mappings>
