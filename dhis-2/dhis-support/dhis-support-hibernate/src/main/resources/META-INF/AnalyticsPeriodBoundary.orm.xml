<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.program.AnalyticsPeriodBoundary" metadata-complete="true" cacheable="true">
        <table name="periodboundary"/>
        
        <attributes>
            <id name="id">
                <column name="periodboundaryid"/>
            </id>
            <basic name="uid" fetch="EAGER" optional="false">
                <column name="uid" unique="true" nullable="false" length="11"/>
            </basic>
            <basic name="code" fetch="EAGER" optional="true">
                <column name="code" unique="true" nullable="true" length="50"/>
            </basic>
            <basic name="created" fetch="EAGER" optional="false"/>
            <basic name="lastUpdated" fetch="EAGER" optional="false"/>
            <basic name="boundaryTarget" fetch="EAGER" optional="true"/>
            <basic name="analyticsPeriodBoundaryType" fetch="EAGER" optional="true"/>
            <basic name="offsetPeriods" fetch="EAGER" optional="true"/>
            <many-to-one name="lastUpdatedBy" target-entity="org.hisp.dhis.user.User" fetch="LAZY" optional="true">
                <join-column name="lastupdatedby" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_lastupdateby_userid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="offsetPeriodType" target-entity="org.hisp.dhis.period.PeriodType" fetch="LAZY" optional="true">
                <join-column name="offsetperiodtypeid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_periodboundary_periodtype"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="programIndicator" target-entity="org.hisp.dhis.program.ProgramIndicator" fetch="LAZY" optional="true">
                <join-column name="programindicatorid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_periodboundary_programindicator"/>
                <cascade/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
