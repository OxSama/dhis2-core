<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.legend.Legend" metadata-complete="true" cacheable="true">
        <table name="maplegend"/>
        
        <attributes>
            <id name="id">
                <column name="maplegendid"/>
            </id>
            <basic name="uid" fetch="EAGER" optional="false">
                <column name="uid" unique="true" nullable="false" length="11"/>
            </basic>
            <basic name="code" fetch="EAGER" optional="true">
                <column name="code" unique="true" nullable="true" length="50"/>
            </basic>
            <basic name="created" fetch="EAGER" optional="false"/>
            <basic name="lastUpdated" fetch="EAGER" optional="false"/>
            <basic name="name" fetch="EAGER" optional="false">
                <column name="name" unique="false" nullable="false" length="230"/>
            </basic>
            <basic name="startValue" fetch="EAGER" optional="false">
                <column name="startvalue" unique="false" nullable="false"/>
            </basic>
            <basic name="endValue" fetch="EAGER" optional="false">
                <column name="endvalue" unique="false" nullable="false"/>
            </basic>
            <basic name="color" fetch="EAGER" optional="true">
                <column name="color" unique="false"/>
            </basic>
            <basic name="image" fetch="EAGER" optional="true">
                <column name="image" unique="false"/>
            </basic>
            <basic name="translations" fetch="EAGER" optional="true"/>
            <many-to-one name="lastUpdatedBy" target-entity="org.hisp.dhis.user.User" fetch="LAZY" optional="true">
                <join-column name="lastupdatedby" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_lastupdateby_userid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="legendSet" target-entity="org.hisp.dhis.legend.LegendSet" fetch="LAZY" optional="true">
                <join-column name="maplegendsetid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_maplegend_maplegendsetid"/>
                <cascade/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
