<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.expression.Expression" metadata-complete="true">
        <table name="expression"/>
        <dynamic-insert>false</dynamic-insert>
        <dynamic-update>false</dynamic-update>
        <select-before-update>false</select-before-update>
        <caching include="all" access="READ_WRITE"/>
        <batch-size>-1</batch-size>
        <mutable>true</mutable>
        <optimistic-lock>version</optimistic-lock>
        <polymorphism>implicit</polymorphism>
        <attributes>
            <id name="id">
                <column name="expressionid"/>
            </id>
            <basic name="description" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="expression" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="slidingWindow" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="missingValueStrategy" fetch="EAGER" optional="true" optimistic-lock="true">
                <column name="missingvaluestrategy" unique="false" length="100"/>
            </basic>
            <basic name="translations" fetch="EAGER" optional="true" optimistic-lock="true"/>
        </attributes>
    </entity>
</entity-mappings>
