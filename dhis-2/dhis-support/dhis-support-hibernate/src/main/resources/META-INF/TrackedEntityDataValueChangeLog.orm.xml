            </many-to-one>
            <many-to-one name="dataElement" target-entity="org.hisp.dhis.dataelement.DataElement" fetch="LAZY" fetch-mode="SELECT" optional="true">
                <join-column name="dataelementid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_entityinstancedatavalueaudit_dataelementid"/>
                <cascade/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
