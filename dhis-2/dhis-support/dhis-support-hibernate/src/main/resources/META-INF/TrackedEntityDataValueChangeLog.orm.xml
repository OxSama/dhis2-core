<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.trackedentitydatavalue.TrackedEntityDataValueChangeLog" metadata-complete="true">
        <table name="trackedEntityDataValueAudit"/>
        <dynamic-insert>false</dynamic-insert>
        <dynamic-update>false</dynamic-update>
        <select-before-update>false</select-before-update>
        <batch-size>-1</batch-size>
        <mutable>true</mutable>
        <optimistic-lock>version</optimistic-lock>
        <polymorphism>implicit</polymorphism>
        <attributes>
            <id name="id">
                <column name="trackedentitydatavalueauditid"/>
            </id>
            <basic name="value" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="created" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="providedElsewhere" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="modifiedBy" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="auditType" fetch="EAGER" optional="false" optimistic-lock="true">
