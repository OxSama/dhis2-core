<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.relationship.RelationshipType" metadata-complete="true" cacheable="true">
        <table name="relationshiptype"/>
        
        <attributes>
            <id name="id">
                <column name="relationshiptypeid"/>
            </id>
            <basic name="uid" fetch="EAGER" optional="false">
                <column name="uid" unique="true" nullable="false" length="11"/>
            </basic>
            <basic name="code" fetch="EAGER" optional="true">
                <column name="code" unique="true" nullable="true" length="50"/>
            </basic>
            <basic name="created" fetch="EAGER" optional="false"/>
            <basic name="lastUpdated" fetch="EAGER" optional="false"/>
            <basic name="name" fetch="EAGER" optional="false">
                <column name="name" unique="true" nullable="false" length="230"/>
            </basic>
            <basic name="description" fetch="EAGER" optional="true">
                <column name="description" unique="false" nullable="true"/>
            </basic>
            <basic name="translations" fetch="EAGER" optional="true"/>
            <basic name="bidirectional" fetch="EAGER" optional="false">
                <column name="bidirectional" unique="false" nullable="false"/>
            </basic>
            <basic name="referral" fetch="EAGER" optional="false">
                <column name="referral" unique="false" nullable="false"/>
            </basic>
            <basic name="fromToName" fetch="EAGER" optional="false">
                <column name="fromToName" unique="false" nullable="false"/>
            </basic>
            <basic name="toFromName" fetch="EAGER" optional="true">
                <column name="toFromName" unique="false"/>
            </basic>
            <basic name="sharing" fetch="EAGER" optional="true"/>
            <basic name="attributeValues" fetch="EAGER" optional="true"/>
            <many-to-one name="lastUpdatedBy" target-entity="org.hisp.dhis.user.User" fetch="LAZY" optional="true">
                <join-column name="lastupdatedby" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_lastupdateby_userid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="fromConstraint" target-entity="org.hisp.dhis.relationship.RelationshipConstraint" fetch="LAZY" optional="true">
                <join-column name="from_relationshipconstraintid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_relationshiptype_from_relationshipconstraintid"/>
                <cascade>
                    <cascade-all/>
                    <cascade-delete/>
                </cascade>
            </many-to-one>
            <many-to-one name="toConstraint" target-entity="org.hisp.dhis.relationship.RelationshipConstraint" fetch="LAZY" optional="true">
                <join-column name="to_relationshipconstraintid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_relationshiptype_to_relationshipconstraintid"/>
                <cascade>
                    <cascade-all/>
                    <cascade-delete/>
                </cascade>
            </many-to-one>
            <many-to-one name="createdBy" target-entity="org.hisp.dhis.user.User" fetch="LAZY" optional="true">
                <join-column name="userid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_relationshiptype_userid"/>
                <cascade/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
