<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.attribute.Attribute" metadata-complete="true">
        <table name="attribute"/>
        <dynamic-insert>false</dynamic-insert>
        <dynamic-update>false</dynamic-update>
        <select-before-update>false</select-before-update>
        <caching include="all" access="READ_WRITE"/>
        <batch-size>-1</batch-size>
        <mutable>true</mutable>
        <optimistic-lock>version</optimistic-lock>
        <polymorphism>implicit</polymorphism>
        <attributes>
            <id name="id">
                <column name="attributeid"/>
            </id>
            <basic name="uid" fetch="EAGER" optional="false" optimistic-lock="true">
                <column name="uid" unique="true" nullable="false" length="11"/>
            </basic>
            <basic name="code" fetch="EAGER" optional="true" optimistic-lock="true">
                <column name="code" unique="true" nullable="true" length="50"/>
            </basic>
            <basic name="created" fetch="EAGER" optional="false" optimistic-lock="true"/>
            <basic name="lastUpdated" fetch="EAGER" optional="false" optimistic-lock="true"/>
            <basic name="name" fetch="EAGER" optional="false" optimistic-lock="true">
                <column name="name" unique="true" nullable="false" length="230"/>
            </basic>
            <basic name="shortName" fetch="EAGER" optional="true" optimistic-lock="true">
                <column name="shortname" unique="false" nullable="true" length="50"/>
            </basic>
            <basic name="description" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="valueType" fetch="EAGER" optional="false" optimistic-lock="true"/>
            <basic name="mandatory" fetch="EAGER" optional="false" optimistic-lock="true"/>
            <basic name="unique" fetch="EAGER" optional="true" optimistic-lock="true">
                <column name="isunique" unique="false" nullable="true"/>
            </basic>
            <basic name="objectTypes" fetch="EAGER" optional="false" optimistic-lock="true">
                <column name="objecttypes" unique="false" nullable="false"/>
            </basic>
            <basic name="sortOrder" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="translations" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="sharing" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <many-to-one name="lastUpdatedBy" target-entity="org.hisp.dhis.user.User" fetch="LAZY" fetch-mode="SELECT" optional="true">
                <join-column name="lastupdatedby" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_lastupdateby_userid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="optionSet" target-entity="org.hisp.dhis.option.OptionSet" fetch="LAZY" fetch-mode="SELECT" optional="true">
                <join-column name="optionsetid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_attribute_optionsetid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="createdBy" target-entity="org.hisp.dhis.user.User" fetch="LAZY" fetch-mode="SELECT" optional="true">
                <join-column name="userid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_attribute_userid"/>
                <cascade/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
