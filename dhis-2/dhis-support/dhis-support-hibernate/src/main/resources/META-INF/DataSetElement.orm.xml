<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.dataset.DataSetElement" metadata-complete="true" cacheable="true">
        <table name="datasetelement"/>
        
        <attributes>
            <id name="id">
                <column name="datasetelementid"/>
            </id>
            <many-to-one name="dataSet" target-entity="org.hisp.dhis.dataset.DataSet" fetch="LAZY" optional="true">
                <join-column name="datasetid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_datasetelement_datasetid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="dataElement" target-entity="org.hisp.dhis.dataelement.DataElement" fetch="LAZY" optional="false">
                <join-column name="dataelementid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_datasetelement_dataelementid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="categoryCombo" target-entity="org.hisp.dhis.category.CategoryCombo" fetch="LAZY" optional="true">
                <join-column name="categorycomboid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_datasetelement_categorycomboid"/>
                <cascade/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
