<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.relationship.Relationship" metadata-complete="true" cacheable="true">
        <table name="relationship"/>
        
        <attributes>
            <id name="id">
                <column name="relationshipid"/>
            </id>
            <basic name="uid" fetch="EAGER" optional="false">
                <column name="uid" unique="true" nullable="false" length="11"/>
            </basic>
            <basic name="code" fetch="EAGER" optional="true">
                <column name="code" unique="true" nullable="true" length="50"/>
            </basic>
            <basic name="created" fetch="EAGER" optional="false"/>
            <basic name="lastUpdated" fetch="EAGER" optional="false"/>
            <basic name="style" fetch="EAGER" optional="true">
                <column name="style" unique="false"/>
            </basic>
            <basic name="key" fetch="EAGER" optional="false">
                <column name="key" unique="false" nullable="false" length="255"/>
            </basic>
            <basic name="invertedKey" fetch="EAGER" optional="false">
                <column name="inverted_key" unique="false" nullable="false" length="255"/>
            </basic>
            <basic name="deleted" fetch="EAGER" optional="true">
                <column name="deleted" unique="false"/>
            </basic>
            <basic name="createdAtClient" fetch="EAGER" optional="true"/>
            <many-to-one name="lastUpdatedBy" target-entity="org.hisp.dhis.user.User" fetch="LAZY" optional="true">
                <join-column name="lastupdatedby" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_lastupdateby_userid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="relationshipType" target-entity="org.hisp.dhis.relationship.RelationshipType" fetch="LAZY" optional="false">
                <join-column name="relationshiptypeid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_relationship_relationshiptypeid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="from" target-entity="org.hisp.dhis.relationship.RelationshipItem" fetch="LAZY" optional="true">
                <join-column name="from_relationshipitemid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_relationship_from_relationshipitemid"/>
                <cascade>
                    <cascade-all/>
                    
                </cascade>
            </many-to-one>
            <many-to-one name="to" target-entity="org.hisp.dhis.relationship.RelationshipItem" fetch="LAZY" optional="true">
                <join-column name="to_relationshipitemid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_relationship_to_relationshipitemid"/>
                <cascade>
                    <cascade-all/>
                    
                </cascade>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
