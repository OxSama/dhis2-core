<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
    
    
    
    <named-native-query name="getRandomGeneratedValuesNotAvailableNamedQuery">
        <query>SELECT value
        FROM
        trackedentityattributevalue teav
        WHERE
        teav.trackedentityattributeid = :teaId
        and LOWER(teav.value) in (:values)
        UNION
        SELECT value FROM reservedvalue rs
        WHERE
        rs.ownerObject = :ownerObject
        and rs.ownerUid = :ownerUid
        and rs.key = :key
        and LOWER(rs.value) in (:values)</query>
        <cacheable>false</cacheable>
        <read-only>false</read-only>
    </named-native-query>
    <entity class="org.hisp.dhis.reservedvalue.ReservedValue" metadata-complete="true">
        <table name="reservedvalue"/>
        <dynamic-insert>false</dynamic-insert>
        <dynamic-update>false</dynamic-update>
        <select-before-update>false</select-before-update>
        <batch-size>-1</batch-size>
        <mutable>true</mutable>
        <optimistic-lock>version</optimistic-lock>
        <polymorphism>implicit</polymorphism>
        <attributes>
            <id name="id">
                <column name="reservedvalueid"/>
            </id>
            <basic name="expiryDate" fetch="EAGER" optional="false" optimistic-lock="true"/>
            <basic name="created" fetch="EAGER" optional="false" optimistic-lock="true"/>
            <basic name="ownerObject" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="ownerUid" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="key" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="value" fetch="EAGER" optional="true" optimistic-lock="true"/>
        </attributes>
    </entity>
</entity-mappings>
