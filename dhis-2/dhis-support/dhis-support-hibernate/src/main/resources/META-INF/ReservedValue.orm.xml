<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
    <named-native-query name="getRandomGeneratedValuesNotAvailableNamedQuery">
        <query>SELECT value
        FROM
        trackedentityattributevalue teav
        WHERE
        teav.trackedentityattributeid = :teaId
        and LOWER(teav.value) in (:values)
        UNION
        SELECT value FROM reservedvalue rs
        WHERE
        rs.ownerObject = :ownerObject
        and rs.ownerUid = :ownerUid
        and rs.key = :key
        and LOWER(rs.value) in (:values)</query>
    </named-native-query>
    <entity class="org.hisp.dhis.reservedvalue.ReservedValue" metadata-complete="true" cacheable="true">
        <table name="reservedvalue"/>
        
        <attributes>
            <id name="id">
                <column name="reservedvalueid"/>
            </id>
            <basic name="expiryDate" fetch="EAGER" optional="false"/>
            <basic name="created" fetch="EAGER" optional="false"/>
            <basic name="ownerObject" fetch="EAGER" optional="true"/>
            <basic name="ownerUid" fetch="EAGER" optional="true"/>
            <basic name="key" fetch="EAGER" optional="true"/>
            <basic name="value" fetch="EAGER" optional="true"/>
        </attributes>
    </entity>
</entity-mappings>
