<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm" cacheable="true">
 <entity class="org.hisp.dhis.dashboard.DashboardItem" metadata-complete="true">
        <table name="dashboarditem"/>
        
        <attributes>
            <id name="id">
                <column name="dashboarditemid"/>
            </id>
            <basic name="uid" fetch="EAGER" optional="false">
                <column name="uid" unique="true" nullable="false" length="11"/>
            </basic>
            <basic name="code" fetch="EAGER" optional="true">
                <column name="code" unique="true" nullable="true" length="50"/>
            </basic>
            <basic name="created" fetch="EAGER" optional="false"/>
            <basic name="lastUpdated" fetch="EAGER" optional="false"/>
            <basic name="text" fetch="EAGER" optional="true">
                <column name="textcontent" unique="false"/>
            </basic>
            <basic name="translations" fetch="EAGER" optional="true"/>
            <basic name="messages" fetch="EAGER" optional="true"/>
            <basic name="appKey" fetch="EAGER" optional="true"/>
            <basic name="shape" fetch="EAGER" optional="true"/>
            <basic name="x" fetch="EAGER" optional="true"/>
            <basic name="y" fetch="EAGER" optional="true"/>
            <basic name="height" fetch="EAGER" optional="true"/>
            <basic name="width" fetch="EAGER" optional="true"/>
            <many-to-one name="lastUpdatedBy" target-entity="org.hisp.dhis.user.User" fetch="LAZY" optional="true">
                <join-column name="lastupdatedby" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_lastupdateby_userid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="visualization" target-entity="org.hisp.dhis.visualization.Visualization" fetch="LAZY" optional="true">
                <join-column name="visualizationid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_dashboarditem_visualizationid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="eventVisualization" target-entity="org.hisp.dhis.eventvisualization.EventVisualization" fetch="LAZY" optional="true">
                <join-column name="eventvisualizationid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_dashboarditem_evisualizationid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="eventChart" target-entity="org.hisp.dhis.eventchart.EventChart" fetch="LAZY" optional="true">
                <join-column name="eventchartid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_dashboarditem_eventchartid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="map" target-entity="org.hisp.dhis.mapping.Map" fetch="LAZY" optional="true">
                <join-column name="mapid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_dashboarditem_mapid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="eventReport" target-entity="org.hisp.dhis.eventreport.EventReport" fetch="LAZY" optional="true">
                <foreign-key name="fk_dashboarditem_eventreportid"/>
                <cascade/>
            </many-to-one>
            <many-to-many name="users" target-entity="org.hisp.dhis.user.User">
                <order-column name="sort_order"/>
            </many-to-many>
            <many-to-many name="reports" target-entity="org.hisp.dhis.report.Report">
                <order-column name="sort_order"/>
            </many-to-many>
            <many-to-many name="resources" target-entity="org.hisp.dhis.document.Document">
                <order-column name="sort_order"/>
            </many-to-many>
        </attributes>
    </entity>
</entity-mappings>
