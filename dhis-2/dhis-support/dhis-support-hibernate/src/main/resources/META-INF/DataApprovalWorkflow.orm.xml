<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.dataapproval.DataApprovalWorkflow" metadata-complete="true" cacheable="true">
        <table name="dataapprovalworkflow"/>
        
        <attributes>
            <id name="id">
                <column name="workflowid"/>
            </id>
            <basic name="uid" fetch="EAGER" optional="false">
                <column name="uid" unique="true" nullable="false" length="11"/>
            </basic>
            <basic name="code" fetch="EAGER" optional="true">
                <column name="code" unique="true" nullable="true" length="50"/>
            </basic>
            <basic name="created" fetch="EAGER" optional="false"/>
            <basic name="lastUpdated" fetch="EAGER" optional="false"/>
            <basic name="name" fetch="EAGER" optional="false">
                <column name="name" unique="true" nullable="false" length="230"/>
            </basic>
            <basic name="translations" fetch="EAGER" optional="true"/>
            <basic name="sharing" fetch="EAGER" optional="true"/>
            <many-to-one name="lastUpdatedBy" target-entity="org.hisp.dhis.user.User" fetch="LAZY" optional="true">
                <join-column name="lastupdatedby" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_lastupdateby_userid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="periodType" target-entity="org.hisp.dhis.period.PeriodType" fetch="LAZY" optional="false">
                <join-column name="periodtypeid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_dataapprovalworkflow_periodtypeid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="categoryCombo" target-entity="org.hisp.dhis.category.CategoryCombo" fetch="LAZY" optional="true">
                <join-column name="categorycomboid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_dataapprovalworkflow_categorycomboid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="createdBy" target-entity="org.hisp.dhis.user.User" fetch="LAZY" optional="true">
                <join-column name="userid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_dataapprovalworkflow_userid"/>
                <cascade/>
            </many-to-one>
            <one-to-many name="dataSets" target-entity="org.hisp.dhis.dataset.DataSet" orphan-removal="false">
                <sort>unsorted</sort>
                <foreign-key/>
                <cascade/>
            </one-to-many>
            <many-to-many name="levels" target-entity="org.hisp.dhis.dataapproval.DataApprovalLevel">
                <sort>unsorted</sort>
            </many-to-many>
        </attributes>
    </entity>
</entity-mappings>
