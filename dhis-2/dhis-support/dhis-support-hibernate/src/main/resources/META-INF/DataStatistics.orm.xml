            <basic name="eventReportViews" fetch="EAGER" optional="true">
                <column name="eventreportviews" unique="false"/>
            </basic>
            <basic name="eventChartViews" fetch="EAGER" optional="true">
                <column name="eventchartviews" unique="false"/>
            </basic>
            <basic name="eventVisualizationViews" fetch="EAGER" optional="true">
                <column name="eventvisualizationviews" unique="false"/>
            </basic>
            <basic name="dashboardViews" fetch="EAGER" optional="true">
                <column name="dashboardviews" unique="false"/>
            </basic>
            <basic name="passiveDashboardViews" fetch="EAGER" optional="true">
                <column name="passivedashboardviews" unique="false"/>
            </basic>
            <basic name="dataSetReportViews" fetch="EAGER" optional="true">
                <column name="datasetreportviews" unique="false"/>
            </basic>
            <basic name="activeUsers" fetch="EAGER" optional="true">
                <column name="active_users" unique="false"/>
            </basic>
            <basic name="totalViews" fetch="EAGER" optional="true">
                <column name="totalviews" unique="false"/>
            </basic>
            <basic name="savedMaps" fetch="EAGER" optional="true">
