<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.trackedentityattributevalue.TrackedEntityAttributeValue" metadata-complete="true">
        <table name="trackedentityattributevalue"/>
        <dynamic-insert>false</dynamic-insert>
        <dynamic-update>false</dynamic-update>
        <select-before-update>false</select-before-update>
        <batch-size>-1</batch-size>
        <mutable>true</mutable>
        <optimistic-lock>version</optimistic-lock>
        <id-class/>
        <polymorphism>implicit</polymorphism>
        <attributes>
            <basic name="created" fetch="EAGER" optional="false" optimistic-lock="true"/>
            <basic name="lastUpdated" fetch="EAGER" optional="false" optimistic-lock="true"/>
            <basic name="plainValue" fetch="EAGER" optional="true" attribute-accessor="property" optimistic-lock="true">
                <column name="value" unique="false" length="1200"/>
            </basic>
            <basic name="encryptedValue" fetch="EAGER" optional="true" attribute-accessor="property" optimistic-lock="true">
                <column name="encryptedvalue" unique="false" length="50000"/>
            </basic>
            <basic name="storedBy" fetch="EAGER" optional="true" optimistic-lock="true">
                <column name="storedby" unique="false" nullable="true"/>
            </basic>
            <many-to-one name="trackedEntity" target-entity="org.hisp.dhis.trackedentity.TrackedEntity" fetch="LAZY" id="true">
                <join-column name="trackedentityid"/>
                <foreign-key name="fk_attributevalue_trackedentityinstanceid"/>
                <on-delete>no-action</on-delete>
            </many-to-one>
            <many-to-one name="attribute" target-entity="org.hisp.dhis.trackedentity.TrackedEntityAttribute" fetch="LAZY" id="true">
                <join-column name="trackedentityattributeid"/>
                <foreign-key name="fk_attributevalue_trackedentityattributeid"/>
                <on-delete>no-action</on-delete>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
