<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.analytics.AnalyticsTableHook" metadata-complete="true" cacheable="true">
        <table name="tablehook"/>
        
        <attributes>
            <id name="id">
                <column name="analyticstablehookid"/>
            </id>
            <basic name="uid" fetch="EAGER" optional="false">
                <column name="uid" unique="true" nullable="false" length="11"/>
            </basic>
            <basic name="code" fetch="EAGER" optional="true">
                <column name="code" unique="true" nullable="true" length="50"/>
            </basic>
            <basic name="created" fetch="EAGER" optional="false"/>
            <basic name="lastUpdated" fetch="EAGER" optional="false"/>
            <basic name="name" fetch="EAGER" optional="false"/>
            <basic name="phase" fetch="EAGER" optional="false">
                <column name="analyticstablephase" unique="false" nullable="false" length="50"/>
            </basic>
            <basic name="resourceTableType" fetch="EAGER" optional="true">
                <column name="resourcetabletype" unique="false" nullable="true" length="50"/>
            </basic>
            <basic name="analyticsTableType" fetch="EAGER" optional="true">
                <column name="analyticstabletype" unique="false" nullable="true" length="50"/>
            </basic>
            <basic name="sql" fetch="EAGER" optional="false">
                <column name="sql" unique="false" nullable="false"/>
            </basic>
            <many-to-one name="lastUpdatedBy" target-entity="org.hisp.dhis.user.User" fetch="LAZY" optional="true">
                <join-column name="lastupdatedby" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_lastupdateby_userid"/>
                <cascade/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
