<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.version.Version" metadata-complete="true">
        <table name="version"/>
        <dynamic-insert>false</dynamic-insert>
        <dynamic-update>false</dynamic-update>
        <select-before-update>false</select-before-update>
        <caching include="all" access="READ_WRITE"/>
        <batch-size>-1</batch-size>
        <mutable>true</mutable>
        <optimistic-lock>version</optimistic-lock>
        <polymorphism>implicit</polymorphism>
        <attributes>
            <id name="id">
                <column name="versionid"/>
            </id>
            <basic name="key" fetch="EAGER" optional="false" optimistic-lock="true">
                <column name="versionkey" unique="true" nullable="false" length="230"/>
            </basic>
            <basic name="value" fetch="EAGER" optional="true" optimistic-lock="true">
                <column name="versionvalue" unique="false"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
