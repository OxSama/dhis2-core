<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.version.Version" metadata-complete="true" cacheable="true">
        <table name="version"/>
        
        <attributes>
            <id name="id">
                <column name="versionid"/>
            </id>
            <basic name="key" fetch="EAGER" optional="false">
                <column name="versionkey" unique="true" nullable="false" length="230"/>
            </basic>
            <basic name="value" fetch="EAGER" optional="true">
                <column name="versionvalue" unique="false"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
