<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.1.0" xmlns="http://www.hibernate.org/xsd/orm/mapping">
    
    
    
    <entity class="org.hisp.dhis.dataelement.DataElement" metadata-complete="true">
        <table name="dataelement"/>
        
        <attributes>
            <id name="id">
                <column name="dataelementid"/>
            </id>
            <basic name="uid" fetch="EAGER" optional="false">
                <column name="uid" unique="true" nullable="false" length="11"/>
            </basic>
            <basic name="code" fetch="EAGER" optional="true">
                <column name="code" unique="true" nullable="true" length="50"/>
            </basic>
            <basic name="created" fetch="EAGER" optional="false"/>
            <basic name="lastUpdated" fetch="EAGER" optional="false"/>
            <basic name="name" fetch="EAGER" optional="false">
                <column name="name" unique="true" nullable="false" length="230"/>
            </basic>
            <basic name="shortName" fetch="EAGER" optional="false">
                <column name="shortname" unique="true" nullable="false" length="50"/>
            </basic>
            <basic name="description" fetch="EAGER" optional="true"/>
            <basic name="formName" fetch="EAGER" optional="true"/>
            <basic name="style" fetch="EAGER" optional="true">
                <column name="style" unique="false"/>
            </basic>
            <basic name="fieldMask" fetch="EAGER" optional="true"/>
            <basic name="translations" fetch="EAGER" optional="true"/>
            <basic name="valueType" fetch="EAGER" optional="false"/>
            <basic name="valueTypeOptions" fetch="EAGER" optional="true"/>
            <basic name="domainType" fetch="EAGER" optional="false">
                <column name="domainType" unique="false" nullable="false"/>
            </basic>
            <basic name="aggregationType" fetch="EAGER" optional="false">
                <column name="aggregationtype" unique="false" nullable="false" length="50"/>
            </basic>
            <basic name="url" fetch="EAGER" optional="true"/>
            <basic name="zeroIsSignificant" fetch="EAGER" optional="false"/>
            <basic name="attributeValues" fetch="EAGER" optional="true"/>
            <basic name="sharing" fetch="EAGER" optional="true"/>
            <many-to-one name="lastUpdatedBy" target-entity="org.hisp.dhis.user.User" fetch="LAZY" optional="true">
                <join-column name="lastupdatedby" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_lastupdateby_userid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="categoryCombo" target-entity="org.hisp.dhis.category.CategoryCombo" fetch="LAZY" optional="false">
                <join-column name="categorycomboid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_dataelement_categorycomboid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="optionSet" target-entity="org.hisp.dhis.option.OptionSet" fetch="LAZY" optional="true">
                <join-column name="optionsetid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_dataelement_optionsetid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="commentOptionSet" target-entity="org.hisp.dhis.option.OptionSet" fetch="LAZY" optional="true">
                <join-column name="commentoptionsetid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_dataelement_commentoptionsetid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="createdBy" target-entity="org.hisp.dhis.user.User" fetch="LAZY" optional="true">
                <join-column name="userid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_dataelement_userid"/>
                <cascade/>
            </many-to-one>
            <one-to-many name="dataSetElements" target-entity="org.hisp.dhis.dataset.DataSetElement" orphan-removal="false">
              
                <foreign-key name="fk_datasetmembers_dataelementid"/>
                <cascade/>
            </one-to-many>
            <many-to-many name="groups" target-entity="org.hisp.dhis.dataelement.DataElementGroup">
              
            </many-to-many>
            <many-to-many name="legendSets" target-entity="org.hisp.dhis.legend.LegendSet">
                <order-column name="sort_order"/>
            </many-to-many>
            <element-collection name="aggregationLevels">
                <order-column name="sort_order"/>
                <column name="aggregationlevel" unique="false" nullable="true" insertable="true" updatable="true"/>
                <collection-table name="dataelementaggregationlevels"/>
            </element-collection>
        </attributes>
    </entity>
</entity-mappings>
