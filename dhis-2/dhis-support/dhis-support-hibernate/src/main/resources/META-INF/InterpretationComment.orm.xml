<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.interpretation.InterpretationComment" metadata-complete="true">
        <table name="interpretationcomment"/>
        
        <attributes>
            <id name="id">
                <column name="interpretationcommentid"/>
            </id>
            <basic name="uid" fetch="EAGER" optional="true">
                <column name="uid" unique="false" length="11"/>
            </basic>
            <basic name="lastUpdated" fetch="EAGER" optional="false"/>
            <basic name="text" fetch="EAGER" optional="true">
                <column name="commenttext" unique="false"/>
            </basic>
            <basic name="mentions" fetch="EAGER" optional="true"/>
            <basic name="created" fetch="EAGER" optional="false"/>
            <many-to-one name="createdBy" target-entity="org.hisp.dhis.user.User" fetch="LAZY" optional="false">
                <join-column name="userid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_interpretationcomment_userid"/>
                <cascade/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
