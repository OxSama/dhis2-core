<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.program.ProgramStage" metadata-complete="true" cacheable="true">
        <table name="programstage"/>
        
        <attributes>
            <id name="id">
                <column name="programstageid"/>
            </id>
            <basic name="uid" fetch="EAGER" optional="false">
                <column name="uid" unique="true" nullable="false" length="11"/>
            </basic>
            <basic name="code" fetch="EAGER" optional="true">
                <column name="code" unique="true" nullable="true" length="50"/>
            </basic>
            <basic name="created" fetch="EAGER" optional="false"/>
            <basic name="lastUpdated" fetch="EAGER" optional="false"/>
            <basic name="name" fetch="EAGER" optional="false">
                <column name="name" unique="false" nullable="false" length="230"/>
            </basic>
            <basic name="description" fetch="EAGER" optional="true"/>
            <basic name="formName" fetch="EAGER" optional="true"/>
            <basic name="minDaysFromStart" fetch="EAGER" optional="false">
                <column name="mindaysfromstart" unique="false" nullable="false"/>
            </basic>
            <basic name="repeatable" fetch="EAGER" optional="false">
                <column name="repeatable" unique="false" nullable="false"/>
            </basic>
            <basic name="standardInterval" fetch="EAGER" optional="true"/>
            <basic name="executionDateLabel" fetch="EAGER" optional="true">
                <column name="executiondatelabel" unique="false"/>
            </basic>
            <basic name="dueDateLabel" fetch="EAGER" optional="true">
                <column name="duedatelabel" unique="false"/>
            </basic>
            <basic name="programStageLabel" fetch="EAGER" optional="true">
                <column name="programstagelabel" unique="false"/>
            </basic>
            <basic name="eventLabel" fetch="EAGER" optional="true">
                <column name="eventlabel" unique="false"/>
            </basic>
            <basic name="autoGenerateEvent" fetch="EAGER" optional="true"/>
            <basic name="validationStrategy" fetch="EAGER" optional="true">
                <column name="validationstrategy" unique="false" length="32"/>
            </basic>
            <basic name="displayGenerateEventBox" fetch="EAGER" optional="true"/>
            <basic name="generatedByEnrollmentDate" fetch="EAGER" optional="true"/>
            <basic name="blockEntryForm" fetch="EAGER" optional="true"/>
            <basic name="remindCompleted" fetch="EAGER" optional="true"/>
            <basic name="allowGenerateNextVisit" fetch="EAGER" optional="true"/>
            <basic name="openAfterEnrollment" fetch="EAGER" optional="true"/>
            <basic name="reportDateToUse" fetch="EAGER" optional="true"/>
            <basic name="preGenerateUID" fetch="EAGER" optional="true"/>
            <basic name="style" fetch="EAGER" optional="true">
                <column name="style" unique="false"/>
            </basic>
            <basic name="hideDueDate" fetch="EAGER" optional="true"/>
            <basic name="sortOrder" fetch="EAGER" optional="true">
                <column name="sort_order" unique="false"/>
            </basic>
            <basic name="translations" fetch="EAGER" optional="true"/>
            <basic name="featureType" fetch="EAGER" optional="true">
                <column name="featuretype" unique="false"/>
            </basic>
            <basic name="enableUserAssignment" fetch="EAGER" optional="true">
                <column name="enableuserassignment" unique="false"/>
            </basic>
            <basic name="attributeValues" fetch="EAGER" optional="true"/>
            <basic name="sharing" fetch="EAGER" optional="true"/>
            <basic name="referral" fetch="EAGER" optional="false">
                <column name="referral" unique="false" nullable="false"/>
            </basic>
            <many-to-one name="lastUpdatedBy" target-entity="org.hisp.dhis.user.User" fetch="LAZY" optional="true">
                <join-column name="lastupdatedby" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_lastupdateby_userid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="program" target-entity="org.hisp.dhis.program.Program" fetch="LAZY" optional="true">
                <join-column name="programid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_programstage_program"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="dataEntryForm" target-entity="org.hisp.dhis.dataentryform.DataEntryForm" fetch="LAZY" optional="true">
                <join-column name="dataentryformid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_programstage_dataentryform"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="periodType" target-entity="org.hisp.dhis.period.PeriodType" fetch="EAGER" optional="true">
                <join-column name="periodtypeid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_programstage_periodtypeid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="createdBy" target-entity="org.hisp.dhis.user.User" fetch="LAZY" optional="true">
                <join-column name="userid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_programstage_userid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="nextScheduleDate" target-entity="org.hisp.dhis.dataelement.DataElement" fetch="LAZY" optional="true">
                <join-column name="nextscheduledateid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_programstage_nextscheduledateid"/>
                <cascade/>
            </many-to-one>
            <one-to-many name="programStageDataElements" target-entity="org.hisp.dhis.program.ProgramStageDataElement" orphan-removal="true">
                <order-by>sort_order</order-by>
              
                <foreign-key/>
                <cascade>
                    <cascade-all/>
                    <cascade-delete/>
                </cascade>
            </one-to-many>
            <one-to-many name="programStageSections" target-entity="org.hisp.dhis.program.ProgramStageSection" orphan-removal="true">
                <order-by>sortOrder</order-by>
              
                <foreign-key/>
                <cascade>
                    <cascade-all/>
                    <cascade-delete/>
                </cascade>
            </one-to-many>
            <one-to-many name="notificationTemplates" target-entity="org.hisp.dhis.program.notification.ProgramNotificationTemplate" orphan-removal="true">
              
                <foreign-key/>
                <cascade>
                    <cascade-all/>
                    <cascade-delete/>
                </cascade>
            </one-to-many>
        </attributes>
    </entity>
</entity-mappings>
