            <basic name="nationality" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="employer" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="education" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="interests" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="languages" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="welcomeMessage" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="lastCheckedInterpretations" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="whatsApp" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="skype" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="facebookMessenger" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="telegram" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="twitter" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="dataViewMaxOrganisationUnitLevel" fetch="EAGER" optional="true" optimistic-lock="true">
                <column name="dataviewmaxorgunitlevel" unique="false"/>
            </basic>
            <basic name="attributeValues" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="uuid" fetch="EAGER" optional="true" optimistic-lock="true">
                <column name="uuid" unique="true" nullable="true"/>
            </basic>
            <basic name="username" fetch="EAGER" optional="false" optimistic-lock="true">
                <column name="username" unique="true" nullable="false"/>
            </basic>
            <basic name="password" fetch="EAGER" optional="true" optimistic-lock="true">
                <column name="password" unique="false" nullable="true" length="60"/>
            </basic>
            <basic name="secret" fetch="EAGER" optional="true" optimistic-lock="true"/>
            <basic name="externalAuth" fetch="EAGER" optional="true" optimistic-lock="true">
                <column name="externalauth" unique="false"/>
            </basic>
            <basic name="openId" fetch="EAGER" optional="true" optimistic-lock="true">
                <column name="openid" unique="true"/>
            </basic>
            <basic name="ldapId" fetch="EAGER" optional="true" optimistic-lock="true">
                <column name="ldapid" unique="true"/>
