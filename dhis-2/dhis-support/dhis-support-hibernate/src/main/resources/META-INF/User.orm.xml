                <join-column name="lastupdatedby" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_lastupdateby_userid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="avatar" target-entity="org.hisp.dhis.fileresource.FileResource" fetch="LAZY" fetch-mode="SELECT" optional="true">
