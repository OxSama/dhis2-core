<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<entity-mappings version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm">
 <entity class="org.hisp.dhis.indicator.Indicator" metadata-complete="true" cacheable="true">
        <table name="indicator"/>
        
        <attributes>
            <id name="id">
                <column name="indicatorid"/>
            </id>
            <basic name="uid" fetch="EAGER" optional="false">
                <column name="uid" unique="true" nullable="false" length="11"/>
            </basic>
            <basic name="code" fetch="EAGER" optional="true">
                <column name="code" unique="true" nullable="true" length="50"/>
            </basic>
            <basic name="created" fetch="EAGER" optional="false"/>
            <basic name="lastUpdated" fetch="EAGER" optional="false"/>
            <basic name="name" fetch="EAGER" optional="false">
                <column name="name" unique="false" nullable="false" length="230"/>
            </basic>
            <basic name="shortName" fetch="EAGER" optional="false">
                <column name="shortname" unique="false" nullable="false" length="50"/>
            </basic>
            <basic name="description" fetch="EAGER" optional="true"/>
            <basic name="formName" fetch="EAGER" optional="true"/>
            <basic name="annualized" fetch="EAGER" optional="false">
                <column name="annualized" unique="false" nullable="false"/>
            </basic>
            <basic name="decimals" fetch="EAGER" optional="true"/>
            <basic name="translations" fetch="EAGER" optional="true"/>
            <basic name="numerator" fetch="EAGER" optional="false">
                <column name="numerator" unique="false" nullable="false"/>
            </basic>
            <basic name="numeratorDescription" fetch="EAGER" optional="true">
                <column name="numeratordescription" unique="false"/>
            </basic>
            <basic name="denominator" fetch="EAGER" optional="false">
                <column name="denominator" unique="false" nullable="false"/>
            </basic>
            <basic name="denominatorDescription" fetch="EAGER" optional="true">
                <column name="denominatordescription" unique="false"/>
            </basic>
            <basic name="url" fetch="EAGER" optional="true"/>
            <basic name="style" fetch="EAGER" optional="true">
                <column name="style" unique="false"/>
            </basic>
            <basic name="aggregateExportCategoryOptionCombo" fetch="EAGER" optional="true">
                <column name="aggregateexportcategoryoptioncombo" unique="false"/>
            </basic>
            <basic name="aggregateExportAttributeOptionCombo" fetch="EAGER" optional="true">
                <column name="aggregateexportattributeoptioncombo" unique="false"/>
            </basic>
            <basic name="sharing" fetch="EAGER" optional="true"/>
            <basic name="attributeValues" fetch="EAGER" optional="true"/>
            <many-to-one name="lastUpdatedBy" target-entity="org.hisp.dhis.user.User" fetch="LAZY" optional="true">
                <join-column name="lastupdatedby" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_lastupdateby_userid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="indicatorType" target-entity="org.hisp.dhis.indicator.IndicatorType" fetch="LAZY" optional="false">
                <join-column name="indicatortypeid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_indicator_indicatortypeid"/>
                <cascade/>
            </many-to-one>
            <many-to-one name="createdBy" target-entity="org.hisp.dhis.user.User" fetch="LAZY" optional="true">
                <join-column name="userid" unique="false" nullable="true" insertable="true" updatable="true"/>
                <foreign-key name="fk_indicator_userid"/>
                <cascade/>
            </many-to-one>
            <many-to-many name="groups" target-entity="org.hisp.dhis.indicator.IndicatorGroup">
              
            </many-to-many>
            <many-to-many name="dataSets" target-entity="org.hisp.dhis.dataset.DataSet">
              
            </many-to-many>
            <many-to-many name="legendSets" target-entity="org.hisp.dhis.legend.LegendSet">
                <order-column name="sort_order"/>
            </many-to-many>
        </attributes>
    </entity>
</entity-mappings>
